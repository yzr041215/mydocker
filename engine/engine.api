syntax = "v1"

type Request {
	Name string `path:"name,options=you|me"`
}

type Response {
	Message string `json:"message"`
}

type Empty {}

type VersionResponse {
	Version string `json:"version"`
}

@server (
	prefix: /api
	group:  version
)
service engine-api {
	@handler EngineHandler
	get /version (Request) returns (VersionResponse)
}

